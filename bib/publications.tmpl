<table class="table">
<tbody>
	{%- set prev_year = dict() %}
	{% set _ = prev_year.update({'val': 1900}) %}
	{%- for entry in entries %}
	<tr>
		<td>
			<span class="date">
				{% if entry.fields['year'] != prev_year['val'] %}
				<big><strong>{{ entry.fields['year'] }}</strong></big><br />
				{% endif %}
				{% set _ = prev_year.update({'val': entry.fields['year']}) %}
			</span>
		</td>
		<td class="publication">
			<span class="pubtitle">
				{%- set url = entry|main_url %}
				{%- if url %}
					{%- if '.pdf' in url %}
					<img src="img/pdficon_small.png" width="16"
						height="16" alt="PDF" />
					{%- endif %}
				<a href="{{ url|escape }}"><cite>{{ entry|title }}</cite></a>.
				{%- else %}
				<cite>{{ entry|title }}</cite>
				{%- endif %}
			</span><br />
			<span class="authors">
				{{ entry.persons['author']|author_list }}
			</span><br />
			<span class="venuetype">{{ entry|venue_type -}}</span><span class="venue"><cite>{{ entry|venue -}}</cite></span>
			{%- if entry.fields['note'] %}
			<span>
			<br />
			{{ entry.fields['note']}}
			</span>
			{%- endif %}
			<br />
			<span class="links">
			{%- for type, url in (entry|extra_urls).items() %}
				[<a href="{{ url|escape }}">{{ type|escape }}</a>]
			{%- endfor %}
			</span>
		</td>
	</tr>
	{%- endfor %}
</tbody>
</table>
